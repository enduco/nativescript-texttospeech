{
  "name": "nativescript-texttospeech-enduco",
  "version": "1.0.5",
  "description": "A text to speech NativeScript plugin for Android and iOS",
  "main": "texttospeech",
  "typings": "index.d.ts",
  "nativescript": {
    "platforms": {
      "ios": "1.1.2",
      "android": "1.1.0"
    },
    "tns-ios": {
      "version": "2.3.0"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/enduco/nativescript-texttospeech.git"
  },
  "scripts": {
    "tsc": "tsc -skipLibCheck",
    "build": "npm i && tsc",
    "tslint": "cd .. && tslint \"**/*.ts\" --config tslint.json --exclude \"**/node_modules/**\" --exclude \"*demo*/platforms/**\"",
    "ci.tslint": "npm i && tslint '**/*.ts' --config '../tslint.json' --exclude '**/node_modules/**'",
    "demo.ios": "npm run setup && cd ../demo && tns run ios",
    "demo.android": "npm run setup && cd ../demo && tns run android",
    "setup": "npm i && tsc",
    "generate.typings.ios": "cd ../demo && TNS_DEBUG_METADATA_PATH=\"$(pwd)/metadata\" tns build ios && TNS_TYPESCRIPT_DECLARATIONS_PATH=\"$(pwd)/typings\" tns build ios && echo 'Now look for your library typings in demo/typings!'",
    "make-changelog": "cd ../ && github_changelog_generator -u nstudio -p nativescript-texttospeech"
  },
  "keywords": [
    "NativeScript",
    "Text To Speech",
    "Android",
    "iOS",
    "JavaScript"
  ],
  "author": "Anarchic Knight <anarchicknight@gmail.com> (http://www.anarchicknight.co.uk)",
  "contributors": [
    {
      "name": "enduco",
      "email": "bmartin@nstudio.io",
      "url": "https://github.com/BradMartin"
    },
    {
      "name": "Sven Ziegler",
      "email": "info@sven-ziegler.com",
      "url": "https://github.com/svzi"
    },
    {
      "name": "Eddy Verbruggen",
      "url": "https://github.com/EddyVerbruggen"
    },
    {
      "name": "Gaper",
      "url": "https://github.com/apiejh"
    },
    {
      "name": "Josh Sommer",
      "url": "https://github.com/TheOriginalJosh"
    },
    {
      "name": "TJ VanToll",
      "email": "tj.vantoll@gmail.com",
      "url": "https://github.com/tjvantoll"
    },
    {
      "name": "enduco",
      "email": "hello@enduco.app"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/enduco/nativescript-mapbox/issues"
  },
  "homepage": "https://github.com/enduco/nativescript-texttospeech",
  "readmeFilename": "README.md",
  "devDependencies": {
    "tns-core-modules": "^6.4.2",
    "tns-platform-declarations": "^6.4.2",
    "lint-staged": "^10.0.8",
    "husky": "^4.2.3",
    "prettier": "~1.19.1",
    "typescript": "~3.8.3",
    "tslint": "6.1.0",
    "prompt": "^1.0.0",
    "rimraf": "^3.0.2",
    "semver": "^7.1.3"
  },
  "dependencies": {},
  "lint-staged": {
    "**/*.{js, css, ts, json, scss, html, xml, md}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run tslint"
    }
  }
}
